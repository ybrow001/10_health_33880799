<!doctype html>
<html>
  <head>
    <title><%=shopData.shopName%></title>
    <link rel="stylesheet"  type="text/css" href="/main.css" />
  </head>
  <body>
    <h1>search <%=shopData.shopName%></h1>
    <form action="result" method="GET">
      <p>find other meditation practitioners on our site!</p>
      <input id="search-box" type="text" name="search">
      <input type="submit" value="search" >
    </form>
    <div id="result">
      <% if(query_result != null) { %>
        <ul>
          <li> 
            <% if(database_result.length > 0) { %>
              <%= database_result[0].username %>
            <% } else { %>
              search not found
            <% } %> 
          </li>
        </ul>
      <% } %>
    </div>
  </body>
</html>